{
  "name": "I2Solutions_Web_App",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "",
    "heroku-prebuild": "",
    "heroku-postbuild": "./deploy",
    "prueba": "cross-env NODE_ENV=development env-cmd .env  node prueba.js",
    "dev": "cross-env NODE_ENV=development env-cmd .env nodemon server.js --config nodemon.js",
    "db:init": "cross-env NODE_ENV=development env-cmd .env node api/config/dump/fakeGenerator.js",
    "test:m:r": "cross-env NODE_ENV=testing mocha api/movil/test/movil.routes.test.js --slow 5000",
    "test:a": "cross-env NODE_ENV=testing mocha api/test/auth.test.js --slow 5000",
    "test:web": "cross-env NODE_ENV=testing nyc mocha api/web/**/test/*.test.js --recursive --slow 5000",
    "test:local": "cross-env NODE_ENV=testing nyc --reporter=html mocha api/web/**/test/*.test.js api/movil/test/movil.routes.test.js api/test/auth.test.js --recursive --slow 5000",
    "test:all": "cross-env NODE_ENV=testing nyc mocha api/web/**/test/*.test.js api/movil/test/*.test.js api/test/auth.test.js --recursive --slow 5000",
    "test": "npm-run-all lint test:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint app.js server.js api/** scripts/**",
    "docs": "gitbook build .",
    "docs:s": "gitbook serve . "
  },
  "engines": {
    "node": "^9.3.0",
    "yarn": "^1.0.0"
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "ajv-errors": "^1.0.0",
    "body-parser": "^1.18.2",
    "co": "^4.6.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.14.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "ow": "^0.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "rfr": "^1.2.3",
    "sequelize": "^4.37.7",
    "shortid": "^2.2.8",
    "validator": "^9.2.0",
    "vue-resource": "^1.5.1",
    "vuetify": "^1.0.18",
    "vuex": "^3.0.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "env-cmd": "^7.0.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "faker": "^4.1.0",
    "gitbook-cli": "^2.3.2",
    "jsonfile": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "lolex": "^2.7.0",
    "mocha": "^4.1.0",
    "mochawesome": "^3.0.1",
    "nodemon": "^1.13.3",
    "npm-run-all": "^4.1.2",
    "nunjucks": "^3.1.2",
    "nyc": "^11.4.1",
    "sequelize-cli": "^4.0.0",
    "sinon": "^5.0.10",
    "sqlite3": "^4.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "exclude": [
      "api/config/db/index.js",
      "api/config/db/config.js"
    ]
  }
}
