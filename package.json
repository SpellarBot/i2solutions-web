    "lodash": "^4.17.10",
    "mailgun-js": "^0.20.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.7",
    "nodemailer-promise": "^2.0.0",
    "ow": "^0.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "rfr": "^1.2.3",
    "sequelize": "^4.37.7",
    "shortid": "^2.2.8",
