{
  "name": "I2Solutions_Web_App",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "",
    "heroku-prebuild": "./deploy",
    "heroku-postbuild": "",
    "prueba": "cross-env NODE_ENV=development env-cmd .env  node prueba.js",
    "dev": "cross-env NODE_ENV=development env-cmd .env nodemon server.js --config nodemon.js",
    "db:init": "cross-env NODE_ENV=development env-cmd .env node api/config/dump/fakeGenerator.js",
    "test:m:r": "cross-env NODE_ENV=testing mocha api/movil/test/movil.routes.test.js --slow 5000",
    "test:a": "cross-env NODE_ENV=testing mocha api/test/auth.test.js --slow 5000",
    "test:web": "cross-env NODE_ENV=testing nyc mocha api/web/**/test/*.test.js --recursive --slow 5000",
    "test:local": "cross-env NODE_ENV=testing nyc --reporter=html mocha api/web/**/test/*.test.js api/movil/test/movil.routes.test.js api/test/auth.test.js --recursive --slow 5000",
    "test:all": "cross-env NODE_ENV=testing env-cmd .env nyc mocha api/web/**/test/*.test.js api/movil/test/*.test.js api/test/auth.test.js --recursive --slow 5000 --timeout 100000",
    "test": "npm-run-all lint test:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint app.js server.js api/** scripts/**",
    "docs": "gitbook build .",
    "docs:s": "gitbook serve . "
  },
  "dependencies": {
    "ajv": "^6.5.0",
